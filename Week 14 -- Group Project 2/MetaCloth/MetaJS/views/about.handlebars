
<div class="about-header">
 <h1>{{page.header}}</h1>
 <div>
	<p class="about">
	{{page.about}}</p>
	</div>
</div>	

	<div id="employeeList">
	<h1>Our Team</h1>
	{{#each page.employees}}
	<div class-"row">
	<div class="col-md-1">
		{{#if imgPath}}<img class="img-responsive img-rounded" id="bioPic"  src="{{imgPath}}"/>{{else}}<img  class="img-responsive img-rounded" id="bioPic"src="img/batman_placeholder.jpg"/>{{/if}}
	</div>
	<div class="col-md-11">
		<h4>{{name}}&nbsp;|&nbsp;{{position}}</h4>
	<p>{{fullBio}}</p>
	</div>
	</div>	
		{{/each}}
	</div>

	
